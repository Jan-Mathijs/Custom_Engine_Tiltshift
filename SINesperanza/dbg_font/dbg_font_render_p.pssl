#include "dbg_font_render.hs"

float4 main(float4 pos : S_POSITION) : S_TARGET_OUTPUT
{
	uint2 position = (uint2)pos.xy;
	float4 output = renderDebugText(position);
	if(output.w == 0)
		discard;
	return output;
}
 
